	<queryString>
		<![CDATA[SELECT
     candidato.`codigo` AS candidato_codigo,
     candidato.`dataNacimento` AS candidato_dataNacimento,
     candidato.`pessoa` AS candidato_pessoa,
     candidato.`concurso` AS candidato_concurso,
     candidato.`provaEscrita` AS candidato_provaEscrita,
     candidato.`titulo` AS candidato_titulo,
     candidato.`provaDidatica` AS candidato_provaDidatica,
     candidato.`memorial` AS candidato_memorial,
     candidato.`aprovado` AS candidato_aprovado,
     candidato.`compareceuDidatica` AS candidato_compareceuDidatica,
     pessoa.`codigo` AS pessoa_codigo,
     pessoa.`nome` AS pessoa_nome,
     pessoa.`sexo` AS pessoa_sexo,
     configuracaoavaliacaoprova.`codigo` AS configuracaoavaliacaoprova_codigo,
     configuracaoavaliacaoprova.`criterio` AS configuracaoavaliacaoprova_criterio,
     configuracaoavaliacaoprova.`peso` AS configuracaoavaliacaoprova_peso,
     configuracaoavaliacaoprovaescrita.`configuracaoAvaliacaoProva` AS configuracaoavaliacaoprovaescrita_configuracaoAvaliacaoProva,
     configuracaoavaliacaoprovaescrita.`provaEscrita` AS configuracaoavaliacaoprovaescrita_provaEscrita,
     escrita.`codigo` AS escrita_codigo,
     escrita.`pontoSorteado` AS escrita_pontoSorteado,
     escrita.`horaSorteio` AS escrita_horaSorteio,
     escrita.`horaInicio` AS escrita_horaInicio,
     escrita.`horaFinalizacao` AS escrita_horaFinalizacao,
     escrita.`local` AS escrita_local,
     escrita.`horaInicioJugamento` AS escrita_horaInicioJugamento,
     escrita.`localJulgamento` AS escrita_localJulgamento,
     escrita.`localDivulgacaoResultado` AS escrita_localDivulgacaoResultado,
     escrita.`horaInicioResultado` AS escrita_horaInicioResultado,
     escrita.`tema` AS escrita_tema,
     concurso.`codigo` AS concurso_codigo,
     concurso.`edital` AS concurso_edital,
     concurso.`area` AS concurso_area,
     concurso.`classe` AS concurso_classe,
     concurso.`campus` AS concurso_campus,
     concurso.`universidade` AS concurso_universidade,
     concurso.`ministerio` AS concurso_ministerio,
     concurso.`regras` AS concurso_regras,
     concurso.`dataInicio` AS concurso_dataInicio,
     concurso.`banca` AS concurso_banca
FROM
     `pessoa` pessoa INNER JOIN `candidato` candidato ON pessoa.`codigo` = candidato.`pessoa`
     INNER JOIN `escrita` escrita ON candidato.`provaEscrita` = escrita.`codigo`
     INNER JOIN `concurso` concurso ON candidato.`concurso` = concurso.`codigo`
     INNER JOIN `configuracaoavaliacaoprovaescrita` configuracaoavaliacaoprovaescrita ON escrita.`codigo` = configuracaoavaliacaoprovaescrita.`provaEscrita`
     INNER JOIN `configuracaoavaliacaoprova` configuracaoavaliacaoprova ON configuracaoavaliacaoprovaescrita.`configuracaoAvaliacaoProva` = configuracaoavaliacaoprova.`codigo`
WHERE
     candidato.codigo = $P{candidato} and
	concurso.codigo = $P{concurso};]]>
	</queryString>
