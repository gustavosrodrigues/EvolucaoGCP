	<queryString language="SQL">
		<![CDATA[SELECT
     concurso.`area` AS concurso_area,
     concurso.`universidade` AS concurso_universidade,
     concurso.`edital` AS concurso_edital,
     concurso.`classe` AS concurso_classe,
     concurso.`campus` AS concurso_campus,
     concurso.`ministerio` AS concurso_ministerio,
     examinador.`pessoa` AS examinador_pessoa,
     examinador.`concurso` AS examinador_concurso,
     pessoa.`codigo` AS pessoa_codigo,
     pessoa.`nome` AS pessoa_nome
FROM
     `concurso` concurso INNER JOIN `examinador` examinador ON concurso.`codigo` = examinador.`concurso`
     INNER JOIN `pessoa` pessoa ON examinador.`pessoa` = pessoa.`codigo`]]>
