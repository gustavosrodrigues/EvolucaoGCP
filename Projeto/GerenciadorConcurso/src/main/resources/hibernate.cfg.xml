<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/bancoConcursoUnipampa</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">root</property>
    <property name="show_sql">true</property>
    <property name="format_sql">true</property>
    <property name="use_sql_comments">true</property>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Candidato"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Abertura"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Concurso"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Configuracaoavaliacaoprova"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Didatica"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Escrita"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Examinador"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Formacaoacademica"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Memorial"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Pesquisaensinoextensao"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Pessoa"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Producaoartistica"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Registrocronograma"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Tema"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Titulo"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Pesoprovas"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Pesoprovas"/>
  </session-factory>
</hibernate-configuration>
