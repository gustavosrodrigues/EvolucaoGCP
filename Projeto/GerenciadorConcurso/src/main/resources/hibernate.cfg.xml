<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/bancoConcursoUnipampa</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password"></property>
    <property name="show_sql">true</property>
    <property name="format_sql">true</property>
    <property name="use_sql_comments">true</property>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Candidato"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Abertura"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Concurso"/>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Configuracaoavaliacaoprova"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Didatica"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Escrita"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Examinador"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Formacaoacademica"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Memorial"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Pesquisaensinoextensao"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Pessoa"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Producaoartistica"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Registrocronograma"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Tema"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Titulo"></mapping>
    <mapping class="br.edu.unipampa.gerenciadorconcurso.model.Pesoprovas"></mapping>
  </session-factory>
</hibernate-configuration>
